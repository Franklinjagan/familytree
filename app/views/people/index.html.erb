<h1>Listing people</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Born</th>
    <th>Died</th>
    <th>Age</th>
  </tr>

<% @people.each do |person| %>
  <tr>
    <td><%= link_to person.name, person %></td>
    <td><%= person.born %></td>
    <td><%= person.died %></td>
    <td><%= person.age %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Person', new_person_path %>
